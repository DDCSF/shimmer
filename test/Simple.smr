

-- Identity function function.
@id x   = x;
+test   = %test-exp %simple-id
 (@id #nat-5) #nat-3;


-- Duplicate a value.
@dup x  = x x;
+test   = %test-exp %simple-dup
 (@dup %x) (%x %x);


-- Apply a higher order function twice.
@twice f x = f (f x);
+test   = %test-exp %simple-twice
 (@twice (#nat-add #nat'1) #nat'5) #nat'7;


